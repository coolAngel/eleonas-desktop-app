    "package:installer": "node ./script/installer.js"
  },
  "author": "coolAngel",
  "license": "MIT",
  "devDependencies": {
    "electron": "1.8.4",
    "electron-packager": "^12.0.1",
    "electron-winstaller": "^2.6.4",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "electron-settings": "^3.1.4",
    "glob": "^7.1.2"
  }
}
